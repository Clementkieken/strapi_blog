{
  "kind": "collectionType",
  "collectionName": "Article",
  "info": {
    "name": "Article",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "default": "Title",
      "required": true
    },
    "Content": {
      "type": "richtext",
      "default": "Content",
      "required": true
    },
    "Date": {
      "type": "datetime"
    },
    "Chapeau": {
      "type": "text",
      "default": "Chapeau",
      "required": true
    },
    "Media": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "files",
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "writter": {
      "via": "author",
      "model": "writter"
    },
    "category": {
      "via": "tags",
      "model": "category"
    }
  }
}
